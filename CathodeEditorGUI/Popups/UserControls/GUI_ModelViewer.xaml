<UserControl
    x:Class="CommandsEditor.Popups.UserControls.GUI_ModelViewer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hx="http://helix-toolkit.org/wpf/SharpDX"
    xmlns:local="clr-namespace:CommandsEditor.Popups.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Grid DataContext="{Binding RelativeSource={RelativeSource Self}}">
        <hx:Viewport3DX EffectsManager="{Binding EffectsManager}" Camera="{Binding Camera}" Name="viewport">
        </hx:Viewport3DX>
    </Grid>
</UserControl>
